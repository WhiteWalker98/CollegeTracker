<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primary_dark_material_dark</item>
        <item name="colorPrimaryDark">@color/primary_material_dark</item>
        <item name="colorAccent">@color/accent_material_dark</item>
        <item name="android:actionBarStyle">@style/AppBarStyle</item>
    </style>

    <style name="AppBarStyle" parent="android:Widget.Holo.ActionBar.Solid">
        <item name="android:icon">@android:color/transparent </item>
    </style>
    <style name="ThemeOverlay.AppCompat.navTheme">
        <item name="colorPrimary">#abb</item>
        <item name="colorControlHighlight">#1c3a5e</item>
    </style>
</resources>
